 open at 7:00 AM. \r\n\r\n\r\n","venue":{"name":"Houston Athletic Fencing Center","address":"","city":"","state":"","zip":"","country":"USA","timezone":null,"latitude":"","longitude":"","geocode_precision":"none"},"division":{"id":64,"name":"Gulf Coast","abbrev":"GULF","auth":"USFA"},"events":[{"id":2595,"tournament_id":690,"tournament":"Regional Youth Circuit","full_name":"Y10 Mixed Foil","short_name":"Y10MxF","weapon":"Foil","gender":"Mixed","age_limit":"Y10","rating_limit":"Open","rating":"NR","rating_prediction":null,"entries":32,"prereg_count":0,"is_team":false,"description":"","close_of_reg":null,"fee":"0.00"},{"id":2596,"tournament_id":690,"tournament":"Regional Youth Circuit","full_name":"Y10 Mixed Epee","short_name":"Y10MxE","weapon":"Epee","gender":"Mixed","age_limit":"Y10","rating_limit":"Open","rating":"NR","rating_prediction":null,"entries":21,"prereg_count":0,"is_team":false,"description":"","close_of_reg":null,"fee":"0.00"},{"id":2597,"tournament_id":690,"tournament":"Regional Youth Circuit","full_name":"Y10 Mixed Saber","short_name":"Y10MxS","weapon":"Saber","gender":"Mixed","age_limit":"Y10","rating_limit":"Open","rating":"NR","rating_prediction":null,"entries":5,"prereg_count":0,"is_team":false,"description":"","close_of_reg":null,"fee":"0.00"},{"id":2598,"tournament_id":690,"tournament":"Regional Youth Circuit","full_name":"Y12 Mixed Foil","short_name":"Y12MxF","weapon":"Foil","gender":"Mixed","age_limit":"Y12","rating_limit":"Open","rating":"NR","rating_prediction":null,"entries":39,"prereg_count":0,"is_team":false,"description":"","close_of_reg":null,"fee":"0.00"},{"id":2599,"tournament_id":690,"tournament":"Regional Youth Circuit","full_name":"Y12 Mixed Epee","short_name":"Y12MxE","weapon":"Epee","gender":"Mixed","age_limit":"Y12","rating_limit":"Open","rating":"NR","rating_prediction":null,"entries":41,"prereg_count":0,"is_team":false,"description":"","close_of_reg":null,"fee":"0.00"},{"id":2600,"tournament_id":690,"tournament":"Regional Youth