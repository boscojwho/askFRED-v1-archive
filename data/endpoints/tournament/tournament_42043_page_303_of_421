ia","abbrev":"VA","auth":"USFA"},"events":[{"id":150667,"tournament_id":37934,"tournament":"Royal Swords Early Season Shindig","full_name":"Unsanctioned Cadet Mixed Epee","short_name":"CdMxE","weapon":"Epee","gender":"Mixed","authority":{},"age_limit":"Cadet","rating_limit":"Open","rating":"NR","rating_prediction":"","entries":6,"prereg_count":6,"is_team":false,"description":"","close_of_reg":"2017-09-23T08:30:00-04:00","fee":"0.00"},{"id":150668,"tournament_id":37934,"tournament":"Royal Swords Early Season Shindig","full_name":"Unsanctioned Y10Y12 Mixed Foil","short_name":"Y1012MxF","weapon":"Foil","gender":"Mixed","authority":{},"age_limit":"Y10Y12","rating_limit":"Open","rating":"NR","rating_prediction":"","entries":5,"prereg_count":2,"is_team":false,"description":"","close_of_reg":"2017-09-23T08:30:00-04:00","fee":"0.00"},{"id":150669,"tournament_id":37934,"tournament":"Royal Swords Early Season Shindig","full_name":"Unsanctioned Cadet Mixed Foil","short_name":"CdMxF","weapon":"Foil","gender":"Mixed","authority":{},"age_limit":"Cadet","rating_limit":"Open","rating":"NR","rating_prediction":"","entries":6,"prereg_count":6,"is_team":false,"description":"","close_of_reg":"2017-09-23T10:30:00-04:00","fee":"0.00"},{"id":150670,"tournament_id":37934,"tournament":"Royal Swords Early Season Shindig","full_name":"Unsanctioned Y10Y12 Mixed Epee","short_name":"Y1012MxE","weapon":"Epee","gender":"Mixed","authority":{},"age_limit":"Y10Y12","rating_limit":"Open","rating":"NR","rating_prediction":"","entries":5,"prereg_count":4,"is_team":false,"description":"","close_of_reg":"2017-09-23T10:30:00-04:00","fee":"0.00"},{"id":150671,"tournament_id":37934,"tournament":"Royal Swords Early Season Shindig","full_name":"Unsanctioned Y10 Mixed Foil","short_name":"Y10MxF","weapon":"Foil","gender":"Mixed","authority":{},"age_limit":"Y10","rating_limit":"Open","rating":"NR","rating_prediction":"","entries":3,"prereg_count":3,"is_team":false,"description":"","close_of_reg":"2017-09-23T08:30