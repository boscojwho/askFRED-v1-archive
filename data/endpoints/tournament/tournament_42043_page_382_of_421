ute clock. While each point will be awarded, no score will be announced or recorded. Coaches are allowed to coach between each point quietly. \r\n","venue":{"name":"Wasatch Fencing","address":"353 Market Street","city":"Kaysville","state":"UT","zip":"84037","country":"USA","timezone":"America\/Denver","latitude":41.0407306,"longitude":-111.9502463,"geocode_precision":"street"},"division":{"id":12,"name":"Utah-Southern Idaho","abbrev":"UT-SID","auth":"USFA"},"events":[{"id":190081,"tournament_id":48767,"tournament":"Wasatch Cup One","full_name":"EUnder Senior Mixed Epee","short_name":"E-SrMxE","weapon":"Epee","gender":"Mixed","authority":{},"age_limit":"Senior","rating_limit":"EUnder","rating":"E1","rating_prediction":"E1","entries":11,"prereg_count":10,"is_team":false,"description":"","close_of_reg":"2021-09-25T09:00:00-06:00","fee":"10.00"},{"id":190082,"tournament_id":48767,"tournament":"Wasatch Cup One","full_name":"EUnder Senior Mixed Foil","short_name":"E-SrMxF","weapon":"Foil","gender":"Mixed","authority":{},"age_limit":"Senior","rating_limit":"EUnder","rating":"E1","rating_prediction":"E1","entries":17,"prereg_count":16,"is_team":false,"description":"","close_of_reg":"2021-09-25T12:30:00-06:00","fee":"10.00"},{"id":190083,"tournament_id":48767,"tournament":"Wasatch Cup One","full_name":"EUnder Senior Mixed Saber","short_name":"E-SrMxS","weapon":"Saber","gender":"Mixed","authority":{},"age_limit":"Senior","rating_limit":"EUnder","rating":"E1","rating_prediction":"E1","entries":6,"prereg_count":6,"is_team":false,"description":"","close_of_reg":"2021-09-25T15:30:00-06:00","fee":"10.00"},{"id":190084,"tournament_id":48767,"tournament":"Wasatch Cup One","full_name":"Senior Mixed Epee","short_name":"SrMxE","weapon":"Epee","gender":"Mixed","authority":{},"age_limit":"Senior","rating_limit":"Open","rating":"C1","rating_prediction":"C1","entries":15,"prereg_count":15,"is_team":false,"description":"","close_of_reg":"2021-09-25T14:30:00-06:00","fee":"10.00"},{"id":19009