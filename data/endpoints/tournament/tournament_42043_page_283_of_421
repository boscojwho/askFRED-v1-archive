rs must not disturb the order of the bout. The referee and\/or the Bout Committee can decide to expel from the competition venue, with or without warning, any person who by gesture, attitude or language disturbs the good order or smooth running of the tournament. ","venue":{"name":"Stamford Fencing Center","address":"425 Fairfield Ave","city":"Stamford","state":"CT","zip":"06902","country":"USA","timezone":"America\/New_York","latitude":41.0363284,"longitude":-73.5521084,"geocode_precision":"street"},"division":{"id":39,"name":"Connecticut","abbrev":"CT","auth":"USFA"},"events":[{"id":140587,"tournament_id":35495,"tournament":"2016 New England Cup - Youth","full_name":"Y8 Mixed Saber","short_name":"Y8MxS","weapon":"Saber","gender":"Mixed","authority":{},"age_limit":"Y8","rating_limit":"Open","rating":"NR","rating_prediction":"NR","entries":6,"prereg_count":8,"is_team":false,"description":"","close_of_reg":"2016-12-04T10:00:00-05:00","fee":"0.00"},{"id":140588,"tournament_id":35495,"tournament":"2016 New England Cup - Youth","full_name":"Y12 Mixed Saber","short_name":"Y12MxS","weapon":"Saber","gender":"Mixed","authority":{},"age_limit":"Y12","rating_limit":"Open","rating":"NR","rating_prediction":"NR","entries":5,"prereg_count":9,"is_team":false,"description":"","close_of_reg":"2016-12-04T10:00:00-05:00","fee":"0.00"},{"id":140589,"tournament_id":35495,"tournament":"2016 New England Cup - Youth","full_name":"Y10 Mixed Saber","short_name":"Y10MxS","weapon":"Saber","gender":"Mixed","authority":{},"age_limit":"Y10","rating_limit":"Open","rating":"NR","rating_prediction":"NR","entries":8,"prereg_count":14,"is_team":false,"description":"","close_of_reg":"2016-12-04T12:00:00-05:00","fee":"0.00"},{"id":140590,"tournament_id":35495,"tournament":"2016 New England Cup - Youth","full_name":"Y14 Mixed Saber","short_name":"Y14MxS","weapon":"Saber","gender":"Mixed","authority":{},"age_limit":"Y14","rating_limit":"Open","rating":"NR","rating_prediction":"NR","entries":8,"prereg_cou