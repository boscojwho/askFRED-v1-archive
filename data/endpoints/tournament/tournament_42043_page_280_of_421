ar the cruise ship terminals and the waterfront restaurants).\r\nhttp:\/\/www.harborhousepier21.com\/ \r\n\r\nPreregistration opens on 09\/07\/2016.\r\nPreregistration closes on 10\/27\/2016.\r\n\r\n\r\n","venue":{"name":"The Gym at Holy Family School","address":"2601 Avenue N","city":"Galveston","state":"TX","zip":"77550","country":"USA","timezone":"America\/Chicago","latitude":29.294033,"longitude":-94.793756,"geocode_precision":"street"},"division":{"id":64,"name":"Gulf Coast","abbrev":"GULF","auth":"USFA"},"events":[{"id":138003,"tournament_id":34872,"tournament":"Jean Laffite Classic","full_name":"Unrated Senior Mixed Foil","short_name":"U-SrMxF","weapon":"Foil","gender":"Mixed","authority":{},"age_limit":"Senior","rating_limit":"Unrated","rating":"E1","rating_prediction":"E1","entries":12,"prereg_count":8,"is_team":false,"description":"","close_of_reg":"2016-10-29T09:00:00-05:00","fee":"0.00"},{"id":138005,"tournament_id":34872,"tournament":"Jean Laffite Classic","full_name":"Unrated Y10 Mixed Foil","short_name":"U-Y10MxF","weapon":"Foil","gender":"Mixed","authority":{},"age_limit":"Y10","rating_limit":"Unrated","rating":"NR","rating_prediction":"NR","entries":4,"prereg_count":3,"is_team":false,"description":"","close_of_reg":"2016-10-29T09:15:00-05:00","fee":"0.00"},{"id":138006,"tournament_id":34872,"tournament":"Jean Laffite Classic","full_name":"Unrated Y12 Mixed Epee","short_name":"U-Y12MxE","weapon":"Epee","gender":"Mixed","authority":{},"age_limit":"Y12","rating_limit":"Unrated","rating":"NR","rating_prediction":"NR","entries":2,"prereg_count":2,"is_team":false,"description":"","close_of_reg":"2016-10-29T09:15:00-05:00","fee":"0.00"},{"id":138007,"tournament_id":34872,"tournament":"Jean Laffite Classic","full_name":"Unrated Senior Mixed Epee","short_name":"U-SrMxE","weapon":"Epee","gender":"Mixed","authority":{},"age_limit":"Senior","rating_limit":"Unrated","rating":"E1","rating_prediction":"E1","entries":6,"prereg_count":8,"is_team":false,"description"